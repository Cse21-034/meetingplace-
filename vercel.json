{
  "version": 2,
  "buildCommand": "bash build-frontend.sh",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
